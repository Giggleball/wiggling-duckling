html(lang='en')
	head
		meta(http-equiv='Content-Type', content='text/html; charset=UTF-8')
		meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no')
		title Welcome!
		
		link(type='text/css', rel='stylesheet', media='screen,projection', href="includes/materialize.min.css")
		link(type='text/css', rel='stylesheet', media='screen,projection', href="includes/style.css")
		meta(name="viewport" content="width=device-width, initial-scale=1.0")
	body
		nav.white(role='navigation')
			.nav-wrapper.container
				a#logo-container.brand-logo(href='#') GP
					ul.right.hide-on-med-and-down
						li
							a(href='#') Navbar Link
					ul#nav-mobile.side-nav
						li
							a(href='#') Navbar Link
						a.button-collapse(href='#', data-activates='nav-mobile')
						i.material-icons menu
		#index-banner.parallax-container
			.section.no-pad-bot
				.container
					h1.header.center.teal-text.text-lighten-2 Giggleball user informtion app!
					.row.center
				h5.header.col.s12.light A modern responsive front-end framework based on Material Design
					.row.center
						p Search for a user!
							form.search(id='searchCriteria', method='post', action='/users', autocomplete='off')
								label(for='searchString') Search for other Giggleballs:
								input(id='search-box', type='text', name='searchString', list='json-datalist', value='' autofocus='true', placeholder='Search...')
								datalist(id='json-datalist')
					br
					br
			.parallax
			img(src='img/background1.jpg', alt='Unsplashed background img 1')
		.container.center
							h1 Welcome users!
							ul
							each thing in data
								li(style="list-style-type: none;") Welcome #{thing.firstname} 
								br
								br
		footer.page-footer.teal
			.footer-copyright
				
		
		script(type="text/javascript" src="includes/jquery-3.1.1.min.js")
		script(type="text/javascript" src="includes/materialize.min.js")
		script(type="text/javascript" src="includes/init.js")
		script(type="text/javascript" src="includes/main.js")


























































